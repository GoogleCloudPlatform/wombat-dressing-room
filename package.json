{
  "private": true,
  "name": "wombat-dressing-room",
  "repository": "googleapis/wombat-dressing-room",
  "version": "1.1.0",
  "description": "npm registry proxy designed to reduce the attack surface of npm publish",
  "main": "index.js",
  "files": [
    "public/",
    "src/",
    "views/"
  ],
  "scripts": {
    "start": "node -r source-map-support/register build/src/server.js",
    "develop": "cp config/local.env ./.env && npm run compile && npm start",
    "test": "cross-env NODE_ENV=test c8 mocha --recursive build/test/",
    "lint": "gts check && npm run lint-html",
    "lint-html": "prettier -c  **/*.{md,css} *.md",
    "fix": "gts fix && npm run fix-html",
    "fix-html": "prettier --write **/*.{md,css} *.md",
    "clean": "gts clean",
    "compile": "tsc -p . && npm run docs && cp node_modules/github-markdown-css/github-markdown.css public/",
    "docs": "marked ./documentation.md -o views/partials/documentation.hbs && marked ./CHANGELOG.md -o views/partials/changelog.hbs",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "internal-deploy": "cp config/internal.env .env && gcloud app deploy app-internal.yaml --project $GCLOUD_PROJECT",
    "external-deploy": "cp config/external.env .env && gcloud app deploy app-external.yaml --project $GCLOUD_PROJECT",
    "deploy": "npm run compile && npm run internal-deploy && npm run external-deploy",
    "create-indexes": "gcloud --project $GCLOUD_PROJECT datastore indexes create ./index.yaml",
    "coverage": "c8 report --reporter=json && codecov -f coverage/*.json"
  },
  "keywords": [],
  "author": "Google LLC",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/cookie-session": "^2.0.36",
    "@types/express": "^4.16.0",
    "@types/express-handlebars": "^5.0.0",
    "@types/github-url-from-git": "^1.5.0",
    "@types/is-uuid": "^1.0.0",
    "@types/mocha": "^8.0.0",
    "@types/morgan": "^1.7.35",
    "@types/request": "^2.48.1",
    "@types/uuid": "^8.0.0",
    "@types/validate-npm-package-name": "^3.0.0",
    "c8": "^7.3.1",
    "chai": "^4.2.0",
    "codecov": "^3.7.1",
    "cross-env": "^7.0.0",
    "github-markdown-css": "^4.0.0",
    "gts": "^3.0.0",
    "marked": "^2.0.0",
    "mocha": "^8.1.3",
    "nock": "^13.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@google-cloud/datastore": "^6.0.0",
    "@npm/types": "^1.0.1",
    "@otplib/preset-default": "^12.0.0",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "express-handlebars": "^5.0.0",
    "github-url-from-git": "^1.5.0",
    "is-uuid": "^1.0.2",
    "morgan": "^1.9.1",
    "octonode": "^0.10.0",
    "otplib": "^12.0.0",
    "request": "^2.88.0",
    "source-map-support": "^0.5.9",
    "uuid": "^8.0.0",
    "validate-npm-package-name": "^3.0.0"
  }
}
